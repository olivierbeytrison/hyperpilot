    <!-- airport -->
    <style-area fill="#005500" id="airport"/>
    <style-line cap="butt" id="airport" stroke="#005500" width="20"/>

    <!-- base style for fixed width lines -->
    <style-text fill="#ff0000" id="road" k="name" priority="5" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    <style-text fill="#ff0000" id="major-road" k="name" priority="2" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    <style-text fill="#ff0000" id="ref" k="ref" priority="2" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    <style-text caption="true" id="ref-caption" use="ref"/>
    <style-text fill="#ff0000" id="ferry" k="name" size="12" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    <style-text caption="true" dy="20" fill="#ff0000" id="poi" k="name" size="16" stroke="#ffffff" stroke-width="2.0"/>
    <!-- meadow|garden -->
    <!-- grass -->
    <!-- grassland|scrub -->
    <!-- marsh|wetland|mud|nature_reserve -->
    <!-- park|common|green|cemetery|golf_course|dog_park -->
    <style-line cap="butt" fix="true" id="park" stroke="#9ac56e" width="1.0"/>
    <!-- track|footway|path|cycleway -->
    <style-line cap="butt" fix="true" id="footway" stipple="2" stipple-stroke="#d35c48" stipple-width="0.6" stroke="#aaffffff" width="1.8"/>
    <style-line id="highway:cycleway" use="pedestrian"/>
    <style-line id="highway:track" use="pedestrian"/>
    <style-line id="highway:path" use="pedestrian"/>
    <style-line id="footway:z17" use="pedestrian"/>
    <style-line cap="butt" id="bridleway" stipple-stroke="#837b58" stroke="#d3cb98" use="footway" width="0.1"/>
    <style-line cap="butt" fix="true" id="steps" stipple="2" stipple-stroke="#be6253" stipple-width="1.0" stroke="#aaffffff" width="4.2"/>
    <style-line cap="butt" fix="true" id="water:outline" stroke="#a4bbcc" width="1.0"/>
    <style-line cap="butt" fix="true" id="water" stroke="#a4bbcc" width="1.0"/>
    <style-line fix="false" id="river" stroke="#a4bbcc" use="water"/>
    <style-line cap="butt" fix="true" id="fence" stroke="#444444" width="1.2"/>
    <style-line id="building" stroke="#eeb7b6b3" use="fix"/>
    <m e="way" k="natural" v="issea|sea">
        <area mesh="true" use="water"/>
    </m>
    <m e="way" k="natural" v="nosea">
        <area fill="#ffffff" mesh="true"/>
    </m>
    <!-- all closed ways that are not 'highway' or 'building' -->
    <m closed="yes" e="way" k="highway|building" v="~">
        <!-- landuse base -->
        <m k="landuse">
            <m v="urban">
                <area use="greens"/>
            </m>
            <m v="meadow|conservation">
                <area use="greens"/>
            </m>
            <m v="residential|commercial|retail|farmyard">
                <area use="residential"/>
            </m>
        </m>
        <m k="natural" v="grassland|scrub">
            <area use="darkgreen"/>
        </m>
        <m k="landuse">
            <m v="farmland|farm|orchard|vineyard|greenhouse_horticulture|plant_nursery">
                <area use="farmland"/>
            </m>
            <m v="quarry">
                <area use="quarry"/>
            </m>
            <m v="industrial|railway">
                <area use="railway|industrial"/>
            </m>
            <m v="military">
                <area use="military"/>
            </m>
        </m>
        <m k="landuse|natural|leisure|amenity|tourism">
            <!-- kind of more like landuse imho -->
            <m k="leisure|landuse" v="nature_reserve">
                <area use="greens2"/>
                <m zoom-min="14">
                    <line cap="butt" fix="true" stroke="#abe29c" width="1.0"/>
                </m>
            </m>
            <!-- landuse -->
            <m k="landuse" zoom-min="11">
                <!-- how about 'leisure' for this one? -->
                <m v="cemetery">
                    <area use="park"/>
                    <m zoom-min="14">
                        <line use="park"/>
                    </m>
                </m>
            </m>
            <m k="landuse" v="village_green|recreation_ground">
                <area use="greens"/>
            </m>
            <m k="landuse" v="allotments" zoom-min="12">
                <area use="allotments"/>
            </m>
            <m k="leisure" v="park|common|green|golf_course" zoom-min="11">
                <area use="park"/>
                <m zoom-min="14">
                    <line use="park"/>
                </m>
                <!-- <text use="park" />-->
            </m>
            <m k="landuse|natural" v="forest|wood">
                <area use="wood"/>
            </m>
            <!-- keep grass above forest:wood and leisure:park! -->
            <!-- http://wiki.openstreetmap.org/wiki/Proposed_features/conservation,
              often serves as background for leisure=nature_reserve -->
            <m k="landuse" v="grass">
                <area use="lightgreen"/>
            </m>
            <m k="leisure" v="garden">
                <area use="greens"/>
            </m>
            <m k="landuse" v="reservoir|basin">
                <area use="water"/>
            </m>
            <m k="leisure" v="track">
                <line cap="butt" fix="true" stroke="#c1bcb6" width="1.3"/>
            </m>
        </m>
        <!-- end landuse|natural|leisure||amenity|tourism -->
    </m>
    <!-- waterways -->
    <m e="way" k="waterway">
        <m v="ditch|drain" zoom-min="14">
            <line use="water" width="0.2"/>
        </m>
        <m v="canal">
            <line use="river" width="-0.3"/>
        </m>
        <m v="stream" zoom-min="13">
            <line stroke="#99BADF" use="water" width="10"/>
        </m>
        <m select="first" v="river">
            <m zoom-min="12">
                <line use="river" width="0.3"/>
            </m>
            <!-- zoom <= 11 -->
            <m k="rank" v="~|-1" zoom-min="9">
                <line use="water" width="0.2"/>
            </m>
            <m k="rank" v="0|1|2">
                <line use="water" width="0.3"/>
            </m>
            <m k="rank" v="3" zoom-min="3">
                <line use="water" width="0.3"/>
            </m>
            <m k="rank" v="4" zoom-min="4">
                <line use="water" width="0.3"/>
            </m>
            <m k="rank" v="5" zoom-min="5">
                <line use="water" width="0.3"/>
            </m>
            <m k="rank" v="6" zoom-min="6">
                <line use="water" width="0.2"/>
            </m>
            <m k="rank" v="7" zoom-min="7">
                <line use="water" width="0.2"/>
            </m>
            <m k="rank" v="8" zoom-min="8">
                <line use="water" width="0.1"/>
            </m>
            <m k="rank" v="9" zoom-min="9">
                <line use="water" width="0.1"/>
            </m>
        </m>
        <m v="riverbank|dock">
            <area use="water"/>
            <line use="water:outline"/>
        </m>
        <m v="weir">
            <line stroke="#000088" use="fix"/>
        </m>
        <m v="dam" zoom-min="12">
            <line stroke="#ababab" use="fix" width="0.2"/>
        </m>
        <m k="lock" v="yes|true">
            <line stroke="#f8f8f8" use="fix" width="0.5"/>
        </m>
    </m>
    <m e="way">
        <m closed="yes" k="natural" v="water">
            <area use="water"/>
            <caption area-size="0.4" fill="#000000" k="name" size="16" stroke="#aaffffff" stroke-width="2.0"/>
        </m>
        <!-- runways areas -->
        /
        <!-- building -->
        <m k="building|building:part">
            <m k="building">
                <m zoom-min="14">
                    <m closed="yes">
                        <area use="building"/>
                    </m>
                    <m closed="no">
                        <line use="building"/>
                    </m>
                </m>
            </m>
            <m k="building:part">
                <m zoom-min="16">
                    <m closed="yes">
                        <area use="building"/>
                    </m>
                    <m closed="no">
                        <line use="building"/>
                    </m>
                </m>
            </m>
        </m>
        <!-- highway -->
        <m k="highway">
            <m select="first" zoom-max="5" zoom-min="4">
                <m k="area" v="~|no|false">
                    <!-- <m v="secondary|primary_link" zoom-min="9"><line stroke="#f2df6d" width="1.3" cap="butt" fix="true" /></m>-->
                    <m v="route_primary" zoom-min="3">
                        <line cap="butt" fix="true" stroke="#c6c5a2" width="1.2"/>
                    </m>
                    <m v="route_trunk" zoom-min="3">
                        <line cap="butt" fix="true" stroke="#deb683" width="1.3"/>
                    </m>
                    <m v="route_motorway">
                        <line cap="butt" fix="true" stroke="#deb683" width="1.4"/>
                    </m>
                </m>
            </m>
            <m select="first" zoom-max="7" zoom-min="5">
                <m k="area" v="~|no|false">
                    <m v="secondary|primary_link" zoom-min="9">
                        <line stroke="#e2cf5d" use="fix" width="0.3"/>
                    </m>
                    <m v="trunk_link|motorway_link" zoom-min="8">
                        <line stroke="#eec693" use="fix" width="0.4"/>
                    </m>
                    <m v="primary" zoom-min="5">
                        <line stroke="#c6c5a2" use="fix" width="0.5"/>
                    </m>
                    <m v="trunk" zoom-min="5">
                        <line stroke="#c6c5a2" use="fix" width="0.5"/>
                    </m>
                    <m v="motorway">
                        <line stroke="#deb683" use="fix" width="0.6"/>
                    </m>
                </m>
            </m>
            <m zoom-min="8">
                <!-- when tunnel|bridge is present it must be 'no' -->
                <m k="tunnel|bridge" v="~|no">
                    <!-- highway area -->
                    <m k="area" v="yes|true">
                        <m v="footway" zoom-min="15">
                            <line cap="butt" fix="true" stroke="#c0c0c0" width="1.0"/>
                        </m>
                        <m v="pedestrian|service|unclassified|residential|road|living_street" zoom-min="13">
                            <line cap="butt" fix="true" stroke="#d0d0d0" width="1.0"/>
                        </m>
                    </m>
                    <m k="area" v="~|no|false">
                        <m zoom-min="16">
                            <m v="steps">
                                <line use="steps"/>
                            </m>
                            <!-- <m v="track|footway|path|cycleway" zoom-min="16" zoom-max="16"><line use="footway:z16"/></m>-->
                            <m v="track|footway|path|cycleway" zoom-min="17">
                                <line use="footway:z17"/>
                            </m>
                        </m>
                        <m zoom-min="14">
                            <m v="footway">
                                <line use="footway"/>
                            </m>
                            <m v="cycleway">
                                <line use="highway:cycleway"/>
                            </m>
                            <m v="track">
                                <line use="highway:track"/>
                            </m>
                            <m v="path">
                                <line use="highway:path"/>
                            </m>
                            <m v="bridleway">
                                <line use="bridleway"/>
                            </m>
                            <m v="construction">
                                <line use="construction"/>
                            </m>
                            <m v="service">
                                <!-- matches every service but parking_isle -->
                                <m k="service" v="-|parking_aisle">
                                    <line use="highway-service"/>
                                </m>
                                <m k="service" v="parking_aisle" zoom-min="16">
                                    <line use="highway-service" width="-0.4"/>
                                </m>
                            </m>
                        </m>
                        <m v="trunk_link|motorway_link">
                            <line use="trunk_link"/>
                            <text use="major-road"/>
                        </m>
                        <m zoom-min="13">
                            <m v="byway|pedestrian">
                                <line use="residential" width="-0.4"/>
                                <text use="road"/>
                            </m>
                        </m>
                        <m v="residential|road|unclassified|living_street" zoom-min="12">
                            <line use="residential"/>
                            <text use="road"/>
                        </m>
                        <m v="tertiary|secondary_link" zoom-min="12">
                            <line use="tertiary"/>
                            <text use="road"/>
                        </m>
                        <m v="secondary|primary_link" zoom-min="10">
                            <line use="secondary:z11"/>
                            <text use="major-road"/>
                        </m>
                        <m v="primary">
                            <line use="primary:z11"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                        <m v="trunk">
                            <line use="trunk"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                        <m v="motorway">
                            <line use="highway:z11"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                    </m>
                    <!-- end area=~|no|false -->
                </m>
                <!-- end tunnel|bridge=~|no|false -->
                <!-- muse contain bridge -->
                <m k="bridge">
                    <!-- except bridge=no|false -->
                    <m k="bridge" v="-|no|false">
                        <!-- no areas -->
                        <m k="area" v="~|no|false">
                            <m zoom-min="16">
                                <m v="steps">
                                    <line use="steps"/>
                                </m>
                            </m>
                            <m v="track|footway|path|cycleway" zoom-min="15">
                                <line cap="butt" use="footway:z17"/>
                            </m>
                            <m select="first" zoom-min="14">
                                <m v="footway">
                                    <line use="footway"/>
                                </m>
                                <m v="cycleway">
                                    <line use="highway:cycleway"/>
                                </m>
                                <m v="track">
                                    <line use="highway:track"/>
                                </m>
                                <m v="path">
                                    <line use="highway:path"/>
                                </m>
                                <m v="bridleway">
                                    <line use="bridleway"/>
                                </m>
                                <m v="construction">
                                    <line cap="butt" use="construction"/>
                                </m>
                                <m v="service">
                                    <line cap="butt" use="highway-service"/>
                                </m>
                            </m>
                            <m select="first" zoom-min="13">
                                <m v="byway|pedestrian">
                                    <line use="pedestrian:bridge"/>
                                    <text use="road"/>
                                </m>
                                <m v="residential|road|unclassified|living_street">
                                    <line use="residential:bridge"/>
                                    <text use="road"/>
                                </m>
                            </m>
                            <m select="first">
                                <m v="tertiary|secondary_link">
                                    <line cap="butt" use="tertiary"/>
                                    <text use="road"/>
                                </m>
                                <m v="trunk_link|motorway_link">
                                    <line cap="butt" use="trunk_link"/>
                                    <text use="major-road"/>
                                </m>
                                <m v="secondary|primary_link">
                                    <line cap="butt" use="secondary:z11"/>
                                    <text use="major-road"/>
                                </m>
                                <m v="primary">
                                    <line cap="butt" use="primary:z11"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                                <m v="trunk">
                                    <line cap="butt" use="trunk"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                                <m v="motorway">
                                    <line cap="butt" use="highway:z11"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                            </m>
                        </m>
                        <!-- end area=~|no|false -->
                        <m k="area" select="first" v="yes|true">
                            <m v="footway" zoom-min="15">
                                <line cap="butt" stroke="#c0c0c0" width="0.15"/>
                            </m>
                            <m v="pedestrian|service|unclassified|residential|road|living_street" zoom-min="13">
                                <line cap="butt" fix="true" stroke="#c0c0c0" width="1.0"/>
                            </m>
                        </m>
                        <!-- end area=yes|true -->
                    </m>
                    <!-- end bridge=yes -->
                </m>
            </m>
            <!-- end zoom-min=11 -->
        </m>
        <!-- end highway -->
        <!-- runways cores -->

        <!-- man_made features -->
        <m k="man_made" v="pier">
            <m closed="no">
                <line cap="butt" stroke="#d0d0d0" width="0.4"/>
                <line cap="butt" stroke="#e4e4e4" width="0.3"/>
            </m>
        </m>
        <!-- barriers -->
        <m k="barrier">
            <!-- <m v="fence|wall|city_wall" zoom-min="15"><line
              stroke="#909090" width="0.1" cap="butt" /></m>-->
            <m v="retaining_wall" zoom-min="15">
                <line cap="butt" fix="true" stroke="#888888" width="1.1"/>
            </m>
        </m>
        <!-- railway (no tunnel) -->
        <m k="railway" zoom-min="10">
            <m k="tunnel" v="~|false|no">
                <m v="station">
                    <area use="railway"/>
                </m>
                <!-- railway bridge casings -->
                <m zoom-min="14">
                    <m k="bridge" v="yes|true">
                        <m v="tram">
                            <line cap="butt" fix="true" stroke="#777777" width="0.9"/>
                        </m>
                        <m v="subway|light_rail|narrow_gauge">
                            <line cap="butt" fix="true" stroke="#777777" width="0.9"/>
                        </m>
                        <m v="rail">
                            <line cap="butt" fix="true" stroke="#777777" width="0.9"/>
                        </m>
                    </m>
                </m>
                <!-- railway casings and cores -->
                <m v="tram" zoom-min="15">
                    <line fix="true" stroke="#887766" width="1.0"/>
                </m>
                <m v="light_rail|subway|narrow_gauge" zoom-min="14">
                    <line cap="butt" fix="true" stroke="#a0a0a0" width="0.9"/>
                </m>
                <m v="rail|turntable">
                    <line cap="butt" fix="true" stroke="#444444" width="4.0"/>
                </m>
                <m v="rail|turntable" zoom-min="15">
                    <line cap="butt" fix="true" stroke="#444444" width="6.0"/>
                </m>
                <!-- <m v="rail" zoom-max="14" zoom-min="13"><line stroke="#8888aa" width="0.6" cap="butt"
                  fix="true" /></m>-->
                <!-- <m v="rail" zoom-max="13" zoom-min="11"><line stroke="#bbbbcc" width="0.8" cap="butt" fix="true" /></m>-->
                <!-- whatever railway:spur means ... -->
                <m v="disused|spur|abandoned|preserved">
                    <line cap="butt" fix="true" stroke="#cccccc" width="0.8"/>
                </m>
            </m>
        </m>
        <!-- place -->
        <m k="place" v="locality" zoom-min="17">
            <caption fill="#000000" k="name" size="12" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <!-- non-physical routes -->
        <m k="route" v="ferry" zoom-min="12">
            <line cap="butt" dasharray="15,10" stroke="#707070" width="2.0"/>
            <m zoom-min="13">
                <text use="ferry"/>
            </m>
        </m>
        <!-- highway one-way markers -->
        <m k="tunnel" v="~|false|no">
            <m k="area" v="~|false|no">
                <m k="highway">
                    <m k="oneway" v="yes|true" zoom-min="16">
                        <lineSymbol src="symbols/oneway.svg"/>
                    </m>
                </m>
            </m>
        </m>
    </m>
    <!-- end e="way" -->
    <m e="node" select="first">
        <m k="barrier">
            <m zoom-min="16">
                <m v="bollard">
                    <symbol src="symbols/barrier/bollard.svg"/>
                </m>
                <m v="block">
                    <symbol src="symbols/barrier/blocks.svg"/>
                </m>
                <m v="gate">
                    <symbol src="symbols/barrier/gate.svg"/>
                </m>
                <m v="lift_gate">
                    <symbol src="symbols/barrier/lift_gate.svg"/>
                </m>
            </m>
        </m>
        <m k="highway">
            <m v="bus_stop">
                <symbol src="symbols/dot_blue.svg"/>
                <caption dy="20" fill="#0092DA" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
            <m v="traffic_signals" zoom-min="17">
                <symbol src="symbols/transport/traffic_lights.svg"/>
            </m>
            <m v="turning_circle">
                <circle fill="#ffffff" radius="1.4" scale-radius="true"/>
            </m>
        </m>
        <!-- historic -->
        <m k="historic">
            <symbol src="symbols/tourist/monument.svg"/>
            <m zoom-min="17">
                <text use="poi"/>
            </m>
        </m>
        <!-- place -->
        <m k="place">
            <m v="locality" zoom-min="13">
                <caption fill="#ff0000" k="name" priority="5" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
            <m v="suburb" zoom-max="14">
                <caption fill="#ff0000" k="name" priority="4" size="17" stroke="#ffffff" stroke-width="2.0" style="bold_italic"/>
            </m>
            <m v="village" zoom-max="14">
                <caption fill="#ff0000" k="name" priority="3" size="17" stroke="#ffffff" stroke-width="2.0"/>
            </m>
            <m v="island" zoom-min="10">
                <caption fill="#ff0000" k="name" priority="5" size="17" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
            <m v="town">
                <caption fill="#ff0000" k="name" priority="2" size="19" stroke="#ffffff" stroke-width="2.0"/>
            </m>
            <m v="city">
                <m zoom-min="7">
                    <caption dy="14" fill="#ff0000" k="name" priority="1" size="19" stroke="#ffffff" stroke-width="2.0" style="bold" symbol="symbols/dot_black.svg"/>
                </m>
                <m zoom-max="6">
                    <caption dy="14" fill="#ff0000" k="name" priority="1" size="19" stroke="#ffffff" stroke-width="2.0" symbol="symbols/dot_black.svg"/>
                </m>
            </m>
            <m v="country">
                <caption fill="#ff0000" k="name" priority="0" size="20" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
        </m>
        <!-- railway -->
        <m k="railway">
            <m v="station">
                <symbol src="symbols/transport/train_station2.svg"/>
                <caption dy="20" fill="#ec2d2d" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
            <m v="halt|tram_stop">
                <symbol src="symbols/transport/tram_stop.svg"/>
                <caption dy="20" fill="#ec2d2d" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
            </m>
        </m>
    </m>



    <!-- airports -->
    <m k="aeroway">
        <area use="airport"/>
        <line use="airport"/>
        <m v="aerodrome|airport">
            <caption dy="18" fill="#ff0000" k="name" priority="3" size="19" stroke="#ffffff" stroke-width="2.0"/>
            <caption dy="-18" fill="#ff0000" k="ref" priority="3" size="19" stroke="#ffffff" stroke-width="2.0"/>
            <symbol src="symbols/transport/airport2.svg"/>
        </m>
    </m>

    <!-- peak -->
    <m k="natural" v="peak" zoom-min="13">
        <symbol src="symbols/peak.svg" symbol-percent="100"/>
        <caption dy="12" fill="#ff0000" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        <caption dy="-12" fill="#ff0000" k="ele" size="12" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    </m>

    <!-- power lines -->
    <m e="way" k="power" v="line">
        <line use="powerline_big"/>
    </m>
    <m e="way" k="power" v="minor_line">
        <line use="powerline_small"/>
    </m>

    <!-- wind turbines -->
    <m e="node" k="generator:source" v="wind">
        <symbol src="symbols/hyperpilot/wind_turbine.svg" symbol-percent="250"/>
    </m>

    <!-- cable cars -->
    <m e="way" k="aerialway">
        <line use="cablecar"/>
    </m>

</rendertheme>
