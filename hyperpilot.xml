    <!-- base style for fixed width lines -->
    <!--<style-text fill="#ff0000" id="road" k="name" priority="5" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>-->
    <!--<style-text fill="#ff0000" id="major-road" k="name" priority="2" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>-->
    <!--<style-text fill="#ff0000" id="ref" k="ref" priority="2" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>-->
    <!--<style-text caption="true" id="ref-caption" use="ref"/>-->
    <!--<style-text fill="#ff0000" id="ferry" k="name" size="12" stroke="#ffffff" stroke-width="2.0" style="bold"/>-->
    <!--<style-text caption="true" dy="20" fill="#ff0000" id="poi" k="name" size="16" stroke="#ffffff" stroke-width="2.0"/>-->
    <!--<style-line cap="butt" fix="true" id="park" stroke="#9ac56e" width="1.0"/>-->
    <!--&lt;!&ndash; track|footway|path|cycleway &ndash;&gt;-->
    <!--<style-line cap="butt" fix="true" id="footway" stipple="2" stipple-stroke="#d35c48" stipple-width="0.6" stroke="#aaffffff" width="1.8"/>-->
    <!--<style-line id="highway:cycleway" use="pedestrian"/>-->
    <!--<style-line id="highway:track" use="pedestrian"/>-->
    <!--<style-line id="highway:path" use="pedestrian"/>-->
    <!--<style-line id="footway:z17" use="pedestrian"/>-->
    <!--<style-line cap="butt" id="bridleway" stipple-stroke="#837b58" stroke="#d3cb98" use="footway" width="0.1"/>-->
    <!--<style-line cap="butt" fix="true" id="steps" stipple="2" stipple-stroke="#be6253" stipple-width="1.0" stroke="#aaffffff" width="4.2"/>-->
    <!--<style-line cap="butt" fix="true" id="fence" stroke="#444444" width="1.2"/>-->
    <!--<style-line id="building" stroke="#eeb7b6b3" use="fix"/>-->
    <m e="way" k="natural" v="issea|sea">
        <area mesh="true" use="water"/>
    </m>
    <m e="way" k="natural" v="nosea">
        <area fill="#ffffff" mesh="true"/>
    </m>
    <!-- all closed ways that are not 'highway' or 'building' -->
    <m closed="yes" e="way" k="highway|building" v="~">
        <m k="landuse">
            <m v="urban">
                <area use="greens"/>
            </m>
            <m v="meadow|conservation">
                <area use="greens"/>
            </m>
            <m v="residential|commercial|retail|farmyard">
                <area use="residential"/>
            </m>
        </m>
        <m k="natural" v="grassland|scrub">
            <area use="darkgreen"/>
        </m>
        <m k="landuse">
            <m v="farmland|farm|orchard|vineyard|greenhouse_horticulture|plant_nursery">
                <area use="farmland"/>
            </m>
            <m v="quarry">
                <area use="quarry"/>
            </m>
            <m v="industrial|railway">
                <area use="railway|industrial"/>
            </m>
            <m v="military">
                <area use="military"/>
            </m>
        </m>
        <m k="landuse|natural|leisure|amenity|tourism">
            <m k="leisure|landuse" v="nature_reserve">
                <area use="greens2"/>
            </m>
            <m k="landuse" v="village_green|recreation_ground">
                <area use="greens"/>
            </m>
            <m k="landuse" v="allotments" zoom-min="12">
                <area use="allotments"/>
            </m>
            <m k="leisure" v="park|common|green|golf_course" zoom-min="11">
                <area use="park"/>
            </m>
            <m k="landuse|natural" v="forest|wood">
                <area use="wood"/>
            </m>
            <m k="landuse" v="grass">
                <area use="lightgreen"/>
            </m>
            <m k="leisure" v="garden">
                <area use="greens"/>
            </m>

            <m k="leisure" v="track">
                <line cap="butt" fix="true" stroke="#c1bcb6" width="1.3"/>
            </m>
        </m>
    </m>

    <m e="way">

        <!-- runways areas -->
        /
        <!-- highway -->
        <m k="highway">
            <m select="first" zoom-max="5" zoom-min="4">
                <m k="area" v="~|no|false">
                    <!-- <m v="secondary|primary_link" zoom-min="9"><line stroke="#f2df6d" width="1.3" cap="butt" fix="true" /></m>-->
                    <m v="route_primary" zoom-min="3">
                        <line cap="butt" fix="true" stroke="#c6c5a2" width="1.2"/>
                    </m>
                    <m v="route_trunk" zoom-min="3">
                        <line cap="butt" fix="true" stroke="#deb683" width="1.3"/>
                    </m>
                    <m v="route_motorway">
                        <line cap="butt" fix="true" stroke="#deb683" width="1.4"/>
                    </m>
                </m>
            </m>
            <m select="first" zoom-max="7" zoom-min="5">
                <m k="area" v="~|no|false">
                    <m v="secondary|primary_link" zoom-min="9">
                        <line stroke="#e2cf5d" use="fix" width="0.3"/>
                    </m>
                    <m v="trunk_link|motorway_link" zoom-min="8">
                        <line stroke="#eec693" use="fix" width="0.4"/>
                    </m>
                    <m v="primary" zoom-min="5">
                        <line stroke="#c6c5a2" use="fix" width="0.5"/>
                    </m>
                    <m v="trunk" zoom-min="5">
                        <line stroke="#c6c5a2" use="fix" width="0.5"/>
                    </m>
                    <m v="motorway">
                        <line stroke="#deb683" use="fix" width="0.6"/>
                    </m>
                </m>
            </m>
            <m zoom-min="8">
                <!-- when tunnel|bridge is present it must be 'no' -->
                <m k="tunnel|bridge" v="~|no">
                    <!-- highway area -->
                    <m k="area" v="yes|true">
                        <m v="footway" zoom-min="15">
                            <line cap="butt" fix="true" stroke="#c0c0c0" width="1.0"/>
                        </m>
                        <m v="pedestrian|service|unclassified|residential|road|living_street" zoom-min="13">
                            <line cap="butt" fix="true" stroke="#d0d0d0" width="1.0"/>
                        </m>
                    </m>
                    <m k="area" v="~|no|false">
                        <m zoom-min="16">
                            <m v="steps">
                                <line use="steps"/>
                            </m>
                            <!-- <m v="track|footway|path|cycleway" zoom-min="16" zoom-max="16"><line use="footway:z16"/></m>-->
                            <m v="track|footway|path|cycleway" zoom-min="17">
                                <line use="footway:z17"/>
                            </m>
                        </m>
                        <m zoom-min="14">
                            <m v="footway">
                                <line use="footway"/>
                            </m>
                            <m v="cycleway">
                                <line use="highway:cycleway"/>
                            </m>
                            <m v="track">
                                <line use="highway:track"/>
                            </m>
                            <m v="path">
                                <line use="highway:path"/>
                            </m>
                            <m v="bridleway">
                                <line use="bridleway"/>
                            </m>
                            <m v="construction">
                                <line use="construction"/>
                            </m>
                            <m v="service">
                                <!-- matches every service but parking_isle -->
                                <m k="service" v="-|parking_aisle">
                                    <line use="highway-service"/>
                                </m>
                                <m k="service" v="parking_aisle" zoom-min="16">
                                    <line use="highway-service" width="-0.4"/>
                                </m>
                            </m>
                        </m>
                        <m v="trunk_link|motorway_link">
                            <line use="trunk_link"/>
                            <text use="major-road"/>
                        </m>
                        <m zoom-min="13">
                            <m v="byway|pedestrian">
                                <line use="residential" width="-0.4"/>
                                <text use="road"/>
                            </m>
                        </m>
                        <m v="residential|road|unclassified|living_street" zoom-min="12">
                            <line use="residential"/>
                            <text use="road"/>
                        </m>
                        <m v="tertiary|secondary_link" zoom-min="12">
                            <line use="tertiary"/>
                            <text use="road"/>
                        </m>
                        <m v="secondary|primary_link" zoom-min="10">
                            <line use="secondary:z11"/>
                            <text use="major-road"/>
                        </m>
                        <m v="primary">
                            <line use="primary:z11"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                        <m v="trunk">
                            <line use="trunk"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                        <m v="motorway">
                            <line use="highway:z11"/>
                            <text use="major-road"/>
                            <m zoom-max="12">
                                <text use="ref-caption"/>
                            </m>
                            <m zoom-min="13">
                                <text use="ref"/>
                            </m>
                        </m>
                    </m>
                    <!-- end area=~|no|false -->
                </m>
                <!-- end tunnel|bridge=~|no|false -->
                <!-- muse contain bridge -->
                <m k="bridge">
                    <!-- except bridge=no|false -->
                    <m k="bridge" v="-|no|false">
                        <!-- no areas -->
                        <m k="area" v="~|no|false">
                            <m zoom-min="16">
                                <m v="steps">
                                    <line use="steps"/>
                                </m>
                            </m>
                            <m v="track|footway|path|cycleway" zoom-min="15">
                                <line cap="butt" use="footway:z17"/>
                            </m>
                            <m select="first" zoom-min="14">
                                <m v="footway">
                                    <line use="footway"/>
                                </m>
                                <m v="cycleway">
                                    <line use="highway:cycleway"/>
                                </m>
                                <m v="track">
                                    <line use="highway:track"/>
                                </m>
                                <m v="path">
                                    <line use="highway:path"/>
                                </m>
                                <m v="bridleway">
                                    <line use="bridleway"/>
                                </m>
                                <m v="construction">
                                    <line cap="butt" use="construction"/>
                                </m>
                                <m v="service">
                                    <line cap="butt" use="highway-service"/>
                                </m>
                            </m>
                            <m select="first" zoom-min="13">
                                <m v="byway|pedestrian">
                                    <line use="pedestrian:bridge"/>
                                    <text use="road"/>
                                </m>
                                <m v="residential|road|unclassified|living_street">
                                    <line use="residential:bridge"/>
                                    <text use="road"/>
                                </m>
                            </m>
                            <m select="first">
                                <m v="tertiary|secondary_link">
                                    <line cap="butt" use="tertiary"/>
                                    <text use="road"/>
                                </m>
                                <m v="trunk_link|motorway_link">
                                    <line cap="butt" use="trunk_link"/>
                                    <text use="major-road"/>
                                </m>
                                <m v="secondary|primary_link">
                                    <line cap="butt" use="secondary:z11"/>
                                    <text use="major-road"/>
                                </m>
                                <m v="primary">
                                    <line cap="butt" use="primary:z11"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                                <m v="trunk">
                                    <line cap="butt" use="trunk"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                                <m v="motorway">
                                    <line cap="butt" use="highway:z11"/>
                                    <text use="major-road"/>
                                    <m zoom-max="12">
                                        <text use="ref-caption"/>
                                    </m>
                                    <m zoom-min="13">
                                        <text use="ref"/>
                                    </m>
                                </m>
                            </m>
                        </m>
                        <!-- end area=~|no|false -->
                        <m k="area" select="first" v="yes|true">
                            <m v="footway" zoom-min="15">
                                <line cap="butt" stroke="#c0c0c0" width="0.15"/>
                            </m>
                            <m v="pedestrian|service|unclassified|residential|road|living_street" zoom-min="13">
                                <line cap="butt" fix="true" stroke="#c0c0c0" width="1.0"/>
                            </m>
                        </m>
                        <!-- end area=yes|true -->
                    </m>
                    <!-- end bridge=yes -->
                </m>
            </m>
            <!-- end zoom-min=11 -->
        </m>
        <!-- end highway -->
    </m>


    <!-- cities -->
    <m k="place">
        <m v="island" zoom-min="11">
            <caption fill="#000000" k="name" priority="5" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="locality">
            <caption fill="#000000" k="name" priority="5" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="suburb">
            <caption fill="#000000" k="name" priority="4" size="17" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="village">
            <caption fill="#000000" k="name" priority="3" size="17" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="town">
            <caption fill="#000000" k="name" priority="2" size="19" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="city">
            <caption fill="#000000" k="name" priority="1" size="19" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
        <m v="country">
            <caption fill="#000000" k="name" priority="0" size="20" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
    </m>

    <!-- railway -->
    <m k="railway" zoom-min="10">
        <m v="station|halt">
            <symbol src="symbols_new/dot_white.svg" symbol-percent="150"/>
            <caption dy="20" fill="#000000" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>

        <m k="tunnel" v="~|false|no">
            <m v="rail|tram|light_rail|subway|narrow_gauge">
                <line fix="true" cap="butt" stipple="30" stipple-stroke="#cccccc" stroke="#000000" width="4.0"/>
            </m>
        </m>
    </m>

    <!-- bus stops -->
    <m k="highway">
        <m v="bus_stop">
            <symbol src="symbols_new/dot_bus.svg"/>
            <caption dy="20" fill="#0015b6" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        </m>
    </m>

    <!-- ferries -->
    <m k="route" v="ferry" zoom-min="11">
        <line cap="butt" fixed="true" dasharray="15,10" stroke="#707070" width="4.0"/>
    </m>

    <!-- airports -->
    <m k="aeroway" zoom-min="10">
        <area fill="#005500" id="airport"/>
        <line cap="butt" id="airport" stroke="#005500" width="20"/>

        <m v="aerodrome|airport">
            <caption dy="18" fill="#ff0000" k="name" priority="3" size="19" stroke="#ffffff" stroke-width="2.0"/>
            <caption dy="-18" fill="#ff0000" k="ref" priority="3" size="19" stroke="#ffffff" stroke-width="2.0"/>
            <symbol src="symbols_new/airport.svg" />
        </m>
    </m>

    <!-- peaks -->
    <m k="natural" v="peak" zoom-min="13">
        <symbol src="symbols_new/peak.svg"/>
        <caption dy="12" fill="#ff0000" k="name" size="14" stroke="#ffffff" stroke-width="2.0" style="bold"/>
        <caption dy="-12" fill="#ff0000" k="ele" size="12" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    </m>

    <!-- place of worship / churches -->
    <m k="amenity" v="place_of_worship">
        <area fill="#ff0000"/>
    </m>


    <!-- power lines -->
    <m e="way" k="power" v="line">
        <line fix="true" stroke="#ff00f6" stroke-dasharray="20,10" stroke-linecap="butt" stroke-width="3.0"/>
    </m>
    <m e="way" k="power" v="minor_line">
        <line fix="true" stroke="#ff00f6" stroke-dasharray="5,5" stroke-linecap="butt" stroke-width="2.0"/>
    </m>

    <!-- wind turbines -->
    <m e="node" k="generator:source" v="wind">
        <symbol src="symbols_new/wind_turbine.svg" symbol-percent="150"/>
    </m>

    <!-- cable cars -->
    <m e="way" k="aerialway">
        <line fix="true" stroke="#3948e2" stroke-dasharray="20,10" stroke-linecap="butt" stroke-width="3.0"/>
    </m>

    <!-- alpine huts -->
    <m k="tourism" v="alpine_hut">
        <symbol src="symbols_new/alpinehut.svg"/>
    </m>

    <!-- water -->
    <!-- lakes -->
    <m e="way" k="natural" v="water" closed="yes">
        <area fill="#B3DDFF"/>
        <caption area-size="0.4" fill="#5273b2" k="name" size="16" stroke="#ffffff" stroke-width="2.0" style="bold"/>
    </m>
    <m e="way" k="landuse" v="reservoir|basin" closed="yes">
        <area fill="#B3DDFF"/>
    </m>

    <!-- text -->
    <m e="way" k="waterway" v="river" zoom-min="12">
        <pathText k="name" font-style="bold" font-size="14" fill="#5273b2" stroke="#ffffff" stroke-width="2"/>
    </m>

    <m e="way" k="waterway" v="ditch" zoom-min="13">
        <line stroke="#B3DDFF" stroke-width="0.6"/>
    </m>
    <m e="way" k="waterway" v="drain" zoom-min="13">
        <line stroke="#B3DDFF" stroke-width="0.5" stroke-linecap="butt"/>
    </m>
    <m e="way" k="waterway" v="canal" zoom-min="10">
        <line stroke="#B3DDFF" stroke-width="0.9" stroke-linecap="butt"/>
    </m>
    <m e="way" k="waterway" v="stream" zoom-min="13">
        <line stroke="#B3DDFF" stroke-width="0.6"/>
    </m>
    <m e="way" k="waterway" v="river" zoom-min="8">
        <line stroke="#B3DDFF" stroke-width="1.1"/>
    </m>
    <m e="way" k="waterway" v="riverbank" closed="yes">
        <area fill="#B3DDFF"/>
    </m>
    <m e="way" k="waterway" v="riverbank" closed="no">
        <line stroke="#B3DDFF" stroke-width="1"/>
    </m>

    <m e="way" k="natural" v="water" closed="no">
        <line stroke="#B3DDFF" stroke-width="1"/>
    </m>

    <m e="way" k="landuse" v="reservoir|basin" closed="no">
        <line stroke="#B3DDFF" stroke-width="0.2"/>
    </m>
    <m e="way" k="intermittent|seasonal" v="wwi_yes|seas_yes" zoom-min="13">
        <m e="way" k="waterway" v="drain" zoom-min="13">
            <line stroke="#66FFFFFF" stroke-width="0.5" stroke-dasharray="2,8" stroke-linecap="butt"/>
        </m>
        <m e="way" k="waterway" v="canal" zoom-min="10">
            <line stroke="#66FFFFFF" stroke-width="0.9" stroke-dasharray="2,8" stroke-linecap="butt"/>
        </m>
        <m e="way" k="waterway" v="stream|ditch" zoom-min="13">
            <line stroke="#66FFFFFF" stroke-width="0.6" stroke-dasharray="2,8" stroke-linecap="butt"/>
        </m>
        <m e="way" k="waterway" v="river" zoom-min="8">
            <line stroke="#66FFFFFF" stroke-width="1.1" stroke-dasharray="2,8" stroke-linecap="butt"/>
        </m>
    </m>


    <!-- underground waterways -->
    <m e="way" k="tunnel" v="yes" zoom-min="14">
        <m e="way" k="waterway" v="ditch|stream">
            <line stroke="#90FFFFFF" stroke-width="0.4" stroke-dasharray="20,2" stroke-linecap="butt"/>
        </m>
        <m e="way" k="waterway" v="drain">
            <line stroke="#90FFFFFF" stroke-width="0.5" stroke-dasharray="20,2" stroke-linecap="butt"/>
        </m>
        <m e="way" k="waterway" v="canal|river">
            <line stroke="#90FFFFFF" stroke-width="0.9" stroke-dasharray="20,2" stroke-linecap="butt"/>
        </m>
    </m>

</rendertheme>
