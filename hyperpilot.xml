    <!-- areas -->
    <!--<style-area  fill="#f1bfbf" id="military"/>-->
    <!--<style-area  fill="#e8e7e3" id="residential"/>-->
    <!--<style-area  fill="#e6e3e5" id="railway|industrial"/>-->
